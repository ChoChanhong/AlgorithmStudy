package BJ_9095_123더하기;

import java.util.Scanner;

public class Main {
	static int T; // 테스트 케이스의 수
	static int cnt; // 방법의 수

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		T = sc.nextInt();

		for (int tc = 1; tc <= T; tc++) {

			int n = sc.nextInt();

			int sum = 0;
			cnt = 0;

			sol(sum, n);

			System.out.println(cnt);
		}
	}

	static void sol(int sum, int n) {
		if (sum == n) { // 1, 2, 3의 합이 n과 같아지면
			cnt++; // 방법수 ++
			return; // 탈출
		}
		
		else if (sum > n) { // 1, 2, 3의 합이 n보다 커지면 
			return; // 탈출
		}
		
		for (int i = 1; i <= 3; i++) {
			sum += i; // 1, 2, 3 더해봄
			sol(sum, n); // 재귀
			sum -= i; // 다음 숫자
		}
	}
}
